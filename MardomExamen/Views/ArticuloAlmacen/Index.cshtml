@model IEnumerable<MardomExamen.Domain.Entities.vwArticuloAlmaces>

@{
    ViewBag.Title = "Articulos en almacen";
}

<h2>@ViewBag.Title</h2>

<p>
    <a class="btn btn-info" href="/ArticuloAlmacen/Create">Agregar articulo almacen</a>
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Articulo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Almacen)
        </th>
         <th>
            @Html.DisplayNameFor(model => model.Capacidad)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Cantidad)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.Articulo)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Almacen)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Capacidad)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Cantidad)
    </td>
</tr>
}

</table>
